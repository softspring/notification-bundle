{
    "name": "softspring/notification-bundle",
    "description": "User notifications bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "softspring/user-bundle": "^5.1",
        "symfony/console": "^5.4|^6.0|^7.0@dev",
        "symfony/form": "^5.4|^6.0|^7.0@dev",
        "symfony/framework-bundle": "^5.4|^6.0|^7.0@dev",
        "symfony/security-bundle": "^5.4|^6.0|^7.0@dev",
        "symfony/translation": "^5.4|^6.0|^7.0@dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\NotificationBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev",
            "dev-5.2": "5.2.x-dev"
        }
    }
}
